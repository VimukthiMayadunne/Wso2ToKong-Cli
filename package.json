{
  "name": "pizza-cli",
  "version": "0.0.1",
  "description": "An example CLI for ordering pizza's",
  "main": "./lib/index.js",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "sudo npm i -g && pizza",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "keywords": [
    "cli",
    "pizza",
    "typescript",
    "nodejs"
  ],
  "author": "Jeroen Ouwehand",
  "license": "MIT",
  "bin": {
    "pizza": "./lib/index.js"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "clear": "^0.1.0",
    "commander": "^2.19.0",
    "figlet": "^1.2.1",
    "inquirer": "^7.0.0",
    "path": "^0.12.7",
    "read-yaml": "^1.1.0",
    "request": "^2.88.0",
    "requests": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.10",
    "nodemon": "^1.18.7",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
